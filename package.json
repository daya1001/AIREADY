{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:create": "npm run db:push",
    "db:seed": "tsx src/db/seed.ts",
    "db:reset": "tsx src/db/reset-db.ts",
    "db:setup": "npm run db:reset && npm run db:create && npm run db:seed",
    "db:check": "tsx -r dotenv/config check-db.ts",
    "db:set-primary-identifier": "tsx -r dotenv/config src/db/set-primary-identifier.ts",
    "db:hash-passwords": "tsx -r dotenv/config src/db/hash-passwords.ts",
    "server:dev": "tsx -r dotenv/config src/server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\""
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@reduxjs/toolkit": "^2.11.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "express": "^4.19.2",
    "lucide-react": "^0.344.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
