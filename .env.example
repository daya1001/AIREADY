# Neon Auth environment variables for React (Vite)
VITE_STACK_PROJECT_ID='your_stack_project_id'
VITE_STACK_PUBLISHABLE_CLIENT_KEY='your_publishable_key'
STACK_SECRET_SERVER_KEY='your_secret_server_key'

# Database connection strings
# VITE_ prefix is required for Vite to expose to client-side code
VITE_DATABASE_URL='your_neon_database_url'
# Keep DATABASE_URL for Node.js scripts (seeding, etc.)
DATABASE_URL='your_neon_database_url'
